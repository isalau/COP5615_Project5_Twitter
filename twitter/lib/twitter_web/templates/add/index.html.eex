<h1>Subscribe To Someone</h1>

<%= form_for @conn, Routes.add_path(@conn, :addSub), fn f -> %>


  <%= label f, :name %>
  <%= text_input f, :name %>
  <%= error_tag f, :name %>

  <%= label f, :tofollow %>
  <%= text_input f, :tofollow %>
  <%= error_tag f, :tofollow %>

  <div>
    <%= submit "Save" %>
  </div>
<% end %>
